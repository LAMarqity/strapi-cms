{
  "kind": "collectionType",
  "collectionName": "landing_pages",
  "info": {
    "singularName": "landing-page",
    "pluralName": "landing-pages",
    "displayName": "Webinar",
    "description": "Landing pages for webinars and marketing campaigns"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "publishedAt": {
      "type": "datetime",
      "configurable": false,
      "writable": true,
      "visible": false
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "type": "string",
      "maxLength": 200,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 160,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaKeywords": {
      "type": "text",
      "maxLength": 200,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "hero": {
      "type": "component",
      "component": "landing-page.hero",
      "repeatable": false,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "webinarCta": {
      "type": "component",
      "component": "landing-page.webinar-cta",
      "repeatable": false,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "features": {
      "type": "component",
      "component": "landing-page.feature",
      "repeatable": true,
      "max": 12,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuresTitle": {
      "type": "string",
      "default": "Key Features",
      "maxLength": 200,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuresSubtitle": {
      "type": "text",
      "maxLength": 500,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "testimonials": {
      "type": "component",
      "component": "landing-page.testimonial",
      "repeatable": true,
      "max": 10,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "testimonialsTitle": {
      "type": "string",
      "default": "What Our Customers Say",
      "maxLength": 200,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "secondaryCta": {
      "type": "component",
      "component": "landing-page.webinar-cta",
      "repeatable": false,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "footerText": {
      "type": "richtext",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "customCss": {
      "type": "text",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "customJs": {
      "type": "text",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "trackingPixels": {
      "type": "json",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "conversionGoal": {
      "type": "string",
      "maxLength": 100,
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "abTestVariant": {
      "type": "enumeration",
      "default": "control",
      "enum": [
        "control",
        "variant-a",
        "variant-b",
        "variant-c"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
